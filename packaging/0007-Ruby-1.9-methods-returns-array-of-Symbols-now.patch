diff --git a/Examples/test-suite/ruby/ruby_naming_runme.rb b/Examples/test-suite/ruby/ruby_naming_runme.rb
index 8ea5f13..d5525bc 100644
--- a/Examples/test-suite/ruby/ruby_naming_runme.rb
